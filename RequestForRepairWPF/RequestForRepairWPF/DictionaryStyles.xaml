<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="PrimaryDarkColor">
            <Setter Property="ItemsControl.Background" Value="#087f23"/>
        </Style>
        <Style x:Key="PrimaryMidColor">
            <Setter Property="ItemsControl.Background" Value="#4caf50"/>
        </Style>
        <Style x:Key="PrimaryLightColor">
            <Setter Property="ItemsControl.Background" Value="#80e27e"/>
        </Style>

        <Style x:Key="AccentDarkColor">
            <Setter Property="ItemsControl.Background" Value="#c56200"/>
        </Style>
        <Style x:Key="AccentMidColor">
            <Setter Property="ItemsControl.Background" Value="#ff9100"/>
        </Style>
        <Style x:Key="AccentLightColor">
            <Setter Property="ItemsControl.Background" Value="#ffc246"/>
        </Style>

    </ResourceDictionary>